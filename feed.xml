---
layout: null
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
<title>Tyler Deitz's link pool</title>
<link rel="alternate" type="text/html" href="http://tylerdeitz.co" />
<link rel="self" type="application/atom+xml" href="http://tylerdeitz.co/feed.xml" />
<id>http://tylerdeitz.co/feed.xml</id>
<updated>{{ site.time | date_to_rfc822 }}</updated>
<generator>Jekyll v{{ jekyll.version }}</generator>

{% for post in site.posts limit:10 %}
   <entry>
      <title>{{ post.title | xml_escape }}</title>
      {% if post.link %} <link rel="alternate" type="text/html" href="{{ post.link }}" />
      {% else %}         <link rel="alternate" type="text/html" href="http://tylerdeitz.co{{ post.url }}" /> {% endif %}
      <link rel="related" type="text/html" href="http://tylerdeitz.co{{ post.url }}" />
      <published>{{ post.date | date_to_rfc822 }}</published>
      <author>
         <name>Tyler Deitz</name>
         <uri>http://tylerdeitz.co/</uri>
      </author>
      <content type="html" xml:base="http://tylerdeitz.co/" xml:lang="en"> <![CDATA[ {{ post.content }} ]]></content>
   </entry>
{% endfor %}

</feed>
